#include<iostream>
using namespace std;

int main(){

    int i,j,n,x=0,y=1,sum=1;
    cin>>n;

    if(n>=1){

        if(n==1)
    cout<<0;
        
        else{

            cout<<0<<endl;

    for(i=2;i<=n;i++){

        for(j=1;j<=i;j++){

             cout<<sum<<" ";
             sum=x+y;
             x=y;
             y=sum;
             

         }

         cout<<endl;

        }
    }

}

return 0;
}
