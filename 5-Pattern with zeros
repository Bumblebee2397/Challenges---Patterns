#include<iostream>
using namespace std;

int main(){

    int n,i,j,k;
    cin>>n;

   if(n>=1){

        if(n==1)
            cout<<1;

        else if(n==2)
            cout<<1<<endl<<2<<"   "<<2;

        else if(n>=3)
            cout<<1<<endl<<2<<"   "<<2<<endl;

            for(i=3;i<=n;i++){

                    cout<<i<<"   ";
 
                    for(k=1;k<=(i-2);k++){
                        cout<<0<<"   ";
                    }

                    cout<<i<<"   ";
                
                cout<<endl;
            }
            
   }

return 0;
}
